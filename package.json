{
  "name": "strider-github-reporter",
  "version": "1.0.1",
  "description": "Report more than success or failure.",
  "main": "webapp.js",
  "scripts": {
    "test": "mocha -R spec --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixeleet/strider-github-reporter.git"
  },

    "webapp": "webapp.js",
    "icon": "icon.png",
    "config": {
      "controller": "GithubReportController"
    }
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.20.1",
    "sinon-chai": "^2.5.0"
  }
